{
  "name": "wasya_react",
  "version": "0.0.16",
  "description": "Seed project for React apps using ES6 & webpack.",
  "repository": "https://github.com/badsyntax/react-seed",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.1.3",
    "flux": "^2.0.3",
    "foundation-cli": "^2.2.5",
    "google-map-react": "^0.34.0",
    "history": "^4.10.1",
    "material-ui": "^0.17.2",
    "moment": "^2.18.1",
    "normalize.css": "^3.0.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.5.3",
    "react-fa": "^4.2.0",
    "react-facebook-login": "^3.6.2",
    "react-fontawesome": "^1.6.1",
    "react-icons": "^2.2.5",
    "react-materialize": "^1.0.4",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.2.2",
    "react-spinner-material": "^1.1.3",
    "react-stripe-elements": "^1.2.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^1.1.2",
    "react-waypoint": "^6.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "scroll-to-element": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^5.8.3",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^5.3.1",
    "babel-plugin-rewire": "^0.1.8",
    "babel-runtime": "^5.6.15",
    "chai": "^3.0.0",
    "concat-stream": "^1.6.0",
    "css-loader": "^0.15.2",
    "escope": "^3.3.0",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^2.6.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "glob": "^5.0.13",
    "html-loader": "^0.3.0",
    "json-loader": "^0.5.2",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "lodash": "^4.17.13",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "opn": "^3.0.2",
    "phantomjs": "^1.9.17",
    "postcss-loader": "^0.5.1",
    "react-hot-loader": "^1.2.8",
    "react-test-renderer": "^15.5.0",
    "rimraf": "^2.4.1",
    "sass-loader": "^1.0.2",
    "sinon": "^1.15.4",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.3",
    "template-html-loader": "0.0.3",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "globals": {
      "describe": true,
      "it": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "key-spacing": 0,
      "no-script-url": 0,
      "semi": 0,
      "camelcase": 0,
      "no-trailing-spaces": 0,
      "no-multi-spaces": 0,
      "no-unused-vars": 0,
      "comma-dangle": 0,
      "no-undef": 0,
      "space-infix-ops": 0,
      "strict": [
        2,
        "global"
      ],
      "indent": 0,
      "quotes": 0,
      "no-alert": 0,
      "no-underscore-dangle": 0,
      "react/display-name": 0,
      "react/jsx-quotes": 1,
      "react/jsx-no-undef": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1
    }
  },
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "127.0.0.1",
    "devPort": 8000,
    "title": "Wasya Co - Rapid Software Development in the Silicon Valley",
    "static_domain": {
      "development": "static.piousbox.com",
      "production": "static.piousbox.com"
    },
    "asset_path": {
      "development_trash": "/",
      "development": "http://127.0.0.1:8000/",
      "production": "/"
    }
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack && cp -v app/contact_us.php build/contact_us.php && cp -v app/.htaccess build/.htaccess && cp -vr archive build && echo ok",
    "clean": "rm -rf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "lint_custom": "eslint --ext .js --ext .jsx ./app/components/Footer ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  }
}
